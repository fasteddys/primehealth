
@{
    ViewBag.Title = "SerachRequestsForLocked";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section role="main" class="content-body">
    <!--Start Header-->
    <header class="page-header">
        <h2>Requests UnLocked</h2>
        <div class="right-wrapper text-right">
            <ol class="breadcrumbs">
                <li><span>Provider</span></li>
                <li><span>Requests UnLocked</span></li>
            </ol>
            <div class="sidebar-right-toggle"><i class=""></i></div>
        </div>
    </header>
    <!--Start Card-->
    <section class="card">
        <header class="card-header">
            <h2 class="card-title">Requests UnLocked</h2>
        </header>
        <div class="card-body">
            <div class="formSearchAboutRequestsUnLocked">
                <div id="RequestsUnLocked" class="tab-pane active">
                    <div class="form-group row">
                        <div class="col-lg-3 col-md-6 ">
                            <label class="col-lg-10 control-label">From<span class="red">*</span></label>
                            <input id="RequestsUnLockedFrom" type="text" @*data-plugin-datepicker=""*@ class="form-control">
                        </div>
                        <div class="col-lg-3 col-md-6">
                            <label class="col-lg-10 control-label">To<span class="red">*</span></label>
                            <input id="RequestsUnLockedTo" type="text" @*data-plugin-datepicker=""*@ class="form-control">
                        </div>
                        <div class="col-lg-3 col-md-6">
                            <label class="col-lg-10 control-label">Entity<span class="red">*</span></label>
                            @*<select data-plugin-selectTwo class="form-control populate" id="RequestsUnLockedEntites">*@
                            <select id="RequestsUnLockedEntites" class="form-control">
                                <option value="default" disabled selected>Select Entites</option>
                                @*<option>ReceivingRequest</option>
                    <option>FilterationRequest</option>
                    <option>BatchRequest</option>*@
                            </select>
                        </div>
                        <div class="col-lg-3 col-md-6">
                            <label class="col-lg-10 control-label">Status</label>
                            <select id="RequestsUnLockedStatus" class="form-control">
                                <option value="default" disabled selected>Select Status</option>
                                @*<optgroup  label="Select Status"></optgroup>*@
                                @*<option>Printed</option>
                    <option>DataEntry Admin Assign</option>
                    <option>New Closing Request</option>*@
                            </select>
                        </div>
                        <div class="col-md-12 mt-3">
                            <button class="btn btn-primary" id="btngetAllUnlockedRequest">Search</button>
                        </div>
                    </div>
                    <div id="showData">

                    </div>
                    <table class="table table-bordered table-striped mb-0" id="SearchResultTable"></table>
                    <!--Start Table-->
                    @*coder here table*@
                    <!--End table-->
                </div>
            </div>
        </div>
    </section>
    <!--End Card-->
    <!--End Header-->
</section>

<script src="~/CustomJavaScript/ProviderJS/SerachRequestsForLocked.js"></script>
<!--End Html-->
<style>
    #btngetAllUnlockedRequest {
        display: block;
        margin: 0 auto;
        width: 40%;
        border-radius: 20px;
        padding: 10px;
        font-size: x-large;
    }
</style>

<script>
    $("#RequestsUnLockedFrom").datepicker({
        dateFormat: 'dd/mm/yy',
    });

    $("#RequestsUnLockedTo").datepicker({

        dateFormat: 'dd/mm/yy',

    });

</script>