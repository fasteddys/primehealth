<!--Start Html-->
<section role="main" class="content-body">
    <header class="page-header">
        <h2>Configuration</h2>
        <div class="right-wrapper text-right">
            <ol class="breadcrumbs">
                <li>
                    <a href="#">
                        <i class="fas fa-cogs"></i>
                    </a>
                </li>
                <li><span>Configuration</span></li>
                <li><span>Holidays</span></li>
            </ol>
        </div>
    </header>
    <section class="card">
        <header class="card-header">
            <div class="card-actions">
                <a href="#" class="card-action card-action-toggle" data-card-toggle></a>
                <a href="#" class="card-action card-action-dismiss" data-card-dismiss></a>
            </div>
            <h2 class="card-title">Official Holidays</h2>
        </header>
        <div class="card-body">
            <div class="tab-content">
                <div id="RejectionReasonRequests" class="tab-pane active">
                    <div class="Search">
                        <div class="Cancel">
                            <a class="nav-link" href="#" onclick="GoBack()">
                                <i class="fas fa-undo-alt fa-2x" aria-hidden="true"></i>
                                <span>Cancel</span>
                            </a>
                        </div>
                    </div>  
                    <!--Start Table-->
                    <div class="row">
                        <table class="table table-responsive-md table-hover mb-0" id="RejectionReasonTable">
                            <thead>
                                <tr>
                                    <th>
                                        Official Holidays Id <!--1-->
                                    </th>
                                    <th>
                                        Official Holidays Name  <!--2-->
                                    </th>
                                    <th>
                                        Official  Date  <!--3-->
                                    </th>
                                    <th>
                                        Is Official  <!--8-->
                                    </th>
                                    <th>
                                        Added By <!--4-->
                                    </th>
                                    <th>
                                        Action  <!--5-->
                                    </th>
                        
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                    <!--End table-->
                </div>
            </div>
        </div>
    </section>
    <!--End Card-->
    <!--End Header-->
