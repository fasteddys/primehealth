                            @*<input type="text" class="form-control hasDatepicker" id="">*@
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" id="DeleteHandlerHoursbtn" class=" btn btn-primary" data-dismiss="modal" onclick="">Submit</button>
                    <button type="button" class="btn btn-primary CancelRequestForDeleted" data-dismiss="modal">Cancel</button>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" tabindex="-1" role="dialog" id="ModelDeleteDays" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
