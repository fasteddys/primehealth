    <div class="modal fade" id="DeletionModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div>
                    <h5 class="modal-title" id="exampleModalLabel"></h5>
                </div>
                <div class="modal-body">
                    <div class="form-group">
                        <div id="RequestHandlerTable">
                            <table class="table table-responsive-md table-striped table-hover mb-0">

                                <thead>
                                    <tr class="TrStyle">
                                        <td><b>Date</b></td>
                                        <td><b>Duration</b></td>
                                    </tr>
                                </thead>
                                <tbody id="RequestHandlerTableBody"></tbody>
                            </table>
                            <div class="form-group row ">
                                <br />
                                <br />
                                <label class="col-lg-4 control-label">Revoke Reason</label>
                                <input class="input-group-text col-lg-7" id="DaysDeletionReason" />
                            </div>
                            <div class="center">
                                <br />
                                <button type="button" data-target="#ModelDeleteDays" id="ShowDeleteDaysAlert" disabled data-toggle="modal" class="btn btn-secondary center" data-dismiss="modal">Save</button>

                            </div>
                        </div>

                        <div id="RequestHandlerHoursTable">
                            <table class=" table table-responsive-md table-striped table-hover mb-0">

                                <thead>
                                    <tr class="TrStyle">
                                        <td><b>Date</b></td>
                                        <td><b>Duration</b></td>
                                    </tr>
                                </thead>
                                <tbody id="RequestHandlerHoursTableBody"></tbody>
                            </table>
                            <div class="form-group row center">
                                <br />
                                <br />
                                <label class="col-lg-4 control-label">Deletion Reason</label>
                                <input class="input-group-text col-lg-7" id="HoursDeletionReason" />

                            </div>
                            <div class="center">
                                <br />
                                <button type="button" data-target="#ModelDeleteHours" id="ShowDeleteHoursAlert" disabled data-toggle="modal" class="btn btn-secondary center" data-dismiss="modal">Save</button>

                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="CancelModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div>
                    <h5 class="modal-title" id="exampleModalLabel"></h5>
                </div>
                <div class="modal-content">
                    <div>
                        <h5 class="modal-title" id="exampleModalLabel"></h5>
                    </div>
                    <div class="modal-body">
                        <div class="form-group">
                            <b class="col-lg-6 control-label">
                                <span>
                                    Deletion Reason
                                </span>
                            </b>
                            <input type="text" class="form-control hasDatepicker" placeholder="Deletion Reason" id="CancelReason" />
                        </div>
                        <div class="center">
                            <button class="btn btn-danger" id="CancelRequestbtn" disabled>Delete</button>

                        </div>
                      

                    </div>
                </div>

            </div>
        </div>
    </div>

    <div class="modal fade" tabindex="-1" role="dialog" id="ModelDeleteHours" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Please Confirm</h5>
                </div>
                <div class="modal-body">
                    <div class="form-group row">
                        <div class="col-lg-12">
