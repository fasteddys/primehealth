                                                                    <th><b>User Name</b></th>
                                                                    <th><b>AC ID</b></th>
                                                                    <th><b>Leave Type</b></th>
                                                                    <th><b>Requested Date</b></th>
                                                                    <th><b>Start Date</b></th>
                                                                    <th><b>End Date</b></th>
                                                                    <th><b>Back To Work</b></th>
                                                                    <th><b>Duration</b></th>
                                                                    <th><b>Unit</b></th>
                                                                    <th><b>Partial Unit</b></th>
                                                                    <th><b>Punch In</b></th>
                                                                    <th><b>Punch Out</b></th>
                                                                </tr>
                                                            </thead>
                                                            <tbody id="ApprovalsTable"></tbody>
                                                        </table>
                                                    </div>

                                                </div>
                                            </div>
