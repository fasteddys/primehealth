<div id="ShowSuspendAlert" style="display:none">
    <div class="overlay"></div>
    <div class="Alertstyle Alertstyle-Danger">
        <div class="container">
            <div class="row">
                <div class="col-md-2">
                    <span class="fa fa-check-circle  fa-7x"></span>
                </div>
                <div class="col-md-8">
                    <div class="title text-10">Suspending...</div>
                    <br />
                    <br />
                    <div class="body text-6" id="AlertSuccessText"></div>

                </div>
                <span class="col-md-2">
                    <button class="btn btn-default AlertButtonStyle " id="Suspend" onclick="">Yes</button>
                    <button class="btn btn-default AlertButtonStyle CancelAction">No</button>

                </span>
            </div>
        </div>
    </div>
</div>
