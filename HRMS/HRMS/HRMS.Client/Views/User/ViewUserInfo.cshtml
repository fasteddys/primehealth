<!--Start Html-->
<section role="main" class="content-body">
    <!--Start Header-->
    <header class="page-header">
        <h2>Users Information</h2>
        <div class="right-wrapper text-right">
            <ol class="breadcrumbs">
                <li><span>Users</span></li>
                <li><span>Users Information</span></li>
            </ol>
            <div class="sidebar-right-toggle"><i class=""></i></div>
        </div>
    </header>
    <!--Start Card-->
    <section class="card">
        <header class="card-header">
            <div class="card-actions">
                <a href="#" class="card-action card-action-toggle" data-card-toggle></a>
                <a href="#" class="card-action card-action-dismiss" data-card-dismiss></a>
            </div>
            <h2 class="card-title">Users Information</h2>
        </header>
        <div class="card-body">
            <div class="tab-content">
                <div id="RejectionReasonRequests" class="tab-pane active">
                    <div class="Search">
                        <div class="Cancel">
                            <a class="nav-link" href="#" onclick="GoBack()">
                                <i class="fas fa-undo-alt fa-2x" aria-hidden="true"></i>
                                <span>Cancel</span>
                            </a>
                        </div>

                    </div>
                    <div class="form-group row">
                        <div class="col-lg-4">
                            <label class="col-lg-10 control-label ">Select</label>
                            <select id="InformationUsersSelectBy" data-plugin-selectTwo class="form-control populate">
                                <option value=0 selected>All Users</option>
                                <option value=1 >All Active Users</option>
                                <option value=2 >All Deactive Users</option>
                                <option value=3 >Custom</option>
                            </select>
                        </div>
                        <div class="col-lg-4  divUsersName">
                            <label class="col-lg-10 control-label ">Users<span style="color:red">*</span></label>
                            <select id="UsersListName" multiple data-plugin-selectTwo class="form-control populate">

                            </select>
                        </div>
                        <div class="col-lg-4">
                            <ul class="navbar-nav1">
                                <li class="" data-toggle="modal" data-target="#UserInformationPropertsDataModal" id="FindUsersInformation">
                                    <a href="#" ><span class=" fa fa-search fa-2x" ></span></a>
                                    <label class="col-lg-10 control-label ">Find</label>
                                </li>
                                <li class=" " id="ExtractUsersInformation">
                                    <a href="#"><span class=" fa fa-file-excel fa-2x"></span></a>
                                    <label class="col-lg-10 control-label ">Export Excel</label>
                                </li>
                            </ul>
                            <div class="ClearBoth">
                            </div>
                        </div>
                    </div>
                    <!--Start Table-->
                    <div class="row">
                        <table class="table table-responsive-md table-hover mb-0" id="usersInformationTable">
                            <thead >
                                <tr id="headUserInformation">
                                    @*<th>
                                        UserID<!--1-->
                                    </th>
                                    <th>
                                        UserName  <!--2-->
                                    </th>
    
                                    <th>
                                        PhoneNumber  <!--3-->
                                    </th>*@
                              
                                   
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                    <!--End table-->
                </div>
            </div>
        </div>
        <!--Deleted Reason Modal-->
        <div class="modal fade" id="UserInformationPropertsDataModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h3 class="modal-title alert alert-primary"  id="exampleModalLabel" >Users Information Data </h3>
                    </div>
                    <div class="modal-body">
                         <!-- Default unchecked -->
                        <div class="row">
                            <p class="col-md-7 alert alert-danger text-4 ">Minimum selected 4 and Maximum is</p>
                            <input class="col-md-2 numberOfColumn form-control alert alert-danger" type="text" disabled value="10" />
                            <p class="col-md-3 alert alert-danger text-4 text-center">To Display</p>

                        </div>
                        <div id="PropertyUsersToDisplay" class="custom-control custom-checkbox col-md-6">
                            @*<input type="checkbox" class="checkboxColumn" id="aaa" value="aa" />*@
                            <label></label>
                            @*<input type="checkbox" class="custom-control-input" id="defaultUnchecked" >
                            <label class="custom-control-label" for="defaultUnchecked">Default unchecked</label>
                            <br />*@
                        </div>
                        <button class="btn btn-primary userInformationSearchBtn" id="displayUserInformationData">Display</button>
                    </div>

                </div>
            </div>
        </div>
    </section>
    <!--End Card-->
    <!--End Header-->
</section>


