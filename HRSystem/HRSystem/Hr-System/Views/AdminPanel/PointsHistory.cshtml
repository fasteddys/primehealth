@model IEnumerable<Hr_System.Models.DailyPointsReport>
@{
    ViewBag.Title = "PointsHistory";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="col-md-6 col-sm-6 col-xs-12">
    <div class="x_panel">
        <div class="x_title">
            <h2>@ViewBag.User Daily active Report  <small>Reports</small></h2>
            <ul class="nav navbar-right panel_toolbox">
                <li>
                    <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                </li>
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                    
                </li>
                
            </ul>
            <div class="clearfix"></div>
        </div>
        <div class="x_content">
            <ul class="list-unstyled timeline">
                @foreach (var item in Model)
                {
                    <li>
                    <div class="block">
                        <div class="tags">
                            <a href="" class="tag">
                                <span>@item.RatingDate</span>
                            </a>
                        </div>
                        <div class="block_content">
                            <h2 class="title">
                                <a>@item.Creator add to @item.UserName "@item.RedPoints RedPoints And @item.GreenPoints GreenPoints" </a>
                            </h2>
                            <div class="byline">
                                <span>@item.RatingDate</span> by <a>@item.Creator</a>
                            </div>
                            <p class="excerpt">
                   Comment :@item.Comment
               
                             </p>
                        </div>
                    </div>
                </li>
                }
                
            
            </ul>

        </div>
    </div>
</div>
