@model IEnumerable<EmaTours.DTOs.Business.ClientPickUpDTO>

@{
    ViewBag.Title = "View All Pickup";
}

<!-- Bootstrap Core CSS -->
<link href="~/css/lib/bootstrap/bootstrap.min.css" rel="stylesheet">
<!-- Custom CSS -->
<link href="~/css/helper.css" rel="stylesheet">
<link href="~/css/style.css" rel="stylesheet">
@*<script src="~/js/lib/jquery/jquery.min.js"></script>*@

<!-- Start Page Content -->
<div class="card card-outline-info">
    <div class="card-header">
        @{
            var routeValues = HttpContext.Current.Request.RequestContext.RouteData.Values;
            var ActionName = routeValues["action"].ToString();
            string text = "";
            if (Request.QueryString["RequestStatusID"] == "1")
            {
                text = "New";
            }
            else if (Request.QueryString["RequestStatusID"] == "2")
            {
                text = "Pending";
            }
            else if (Request.QueryString["RequestStatusID"] == "3")
            {
                text = "Closed";
            }

            else if (ActionName == "GetAssignedPickUpRequest")
            {
                text = "My Assigned";
            }
            else
            {
                text = "All";

            }
        }
        <h4 class="m-b-0 text-white">@text PickUps</h4>
    </div>

    <div class="card-body">
        <div class="card">
            <div class="card-body">
                <div class="table-responsive m-t-40">
                    <table id="example23" class="display nowrap table table-hover table-striped table-bordered" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th>PickUp Request ID</th>
                                <th>Creation Date</th>
                                <th>Back Flight Time</th>
                                <th>Number Of Members</th>
                                <th>User Assignee</th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {


                                <tr>
                                    <td>@item.PickUpRequestID</td>
                                    <td>@item.CreationDate</td>
                                    <td>@item.BackFlightTime</td>
                                    <td>@item.NumberOfMembers</td>
                                    <td>@item.UserAssigneeName</td>
                                    <td><a href="~/PickUp/ManagePickupRequest?PickupRequestID=@item.PickUpRequestID">Details</a></td>
                                </tr>
                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End PAge Content -->
<!-- End Wrapper -->
<!-- All Jquery -->
<!-- Bootstrap tether Core JavaScript -->
<script src="~/js/lib/bootstrap/js/popper.min.js"></script>
<script src="~/js/lib/bootstrap/js/bootstrap.min.js"></script>
