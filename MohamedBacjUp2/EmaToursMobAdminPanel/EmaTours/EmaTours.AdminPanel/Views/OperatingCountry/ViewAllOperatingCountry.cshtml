@model IEnumerable<EmaTours.DTOs.Business.OperatingCountryDTO>
@{
    ViewBag.Title = "Operating Countries";
}

<!-- Bootstrap Core CSS -->
<link href="~/css/lib/bootstrap/bootstrap.min.css" rel="stylesheet">
<!-- Custom CSS -->
<link href="~/css/helper.css" rel="stylesheet">
<link href="~/css/style.css" rel="stylesheet">

<!-- Start Page Content -->
<div class="card card-outline-info">
    <div class="card-header">   
        <h4 class="m-b-0 text-white">Operating Countries</h4>
    </div>
    <div class="card">
        <div class="card-body">
            <div class="table-responsive m-t-40">
                <table id="example23" class="display nowrap table table-hover table-striped table-bordered" cellspacing="0" width="100%">
                    <thead>
                        <tr>
                            <th>OperatingCountryID</th>
                            <th>OperatingCountryName</th>

                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {


                            <tr>
                                <td>@item.OperatingCountryID</td>
                                <td>@item.OperatingCountryName</td>

                                <td><a href="~/OperatingCountry/EditOperatingCountry?OperatingCountryID=@item.OperatingCountryID">Edit</a></td>
                            </tr>
                        }

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<!-- End PAge Content -->
<!-- End Wrapper -->
<!-- All Jquery -->
<!-- Bootstrap tether Core JavaScript -->
<script src="~/js/lib/bootstrap/js/popper.min.js"></script>
<script src="~/js/lib/bootstrap/js/bootstrap.min.js"></script>
<!-- slimscrollbar scrollbar JavaScript -->
<script src="~/js/jquery.slimscroll.js"></script>
<!--Menu sidebar -->
<script src="~/js/sidebarmenu.js"></script>
