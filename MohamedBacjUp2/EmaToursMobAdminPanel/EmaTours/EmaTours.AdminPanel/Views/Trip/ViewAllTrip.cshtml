@model IEnumerable<EmaTours.DTOs.Business.TripDTO>

@{
    ViewBag.Title = "View All Trip";
}


<!-- Bootstrap Core CSS -->
<link href="~/css/lib/bootstrap/bootstrap.min.css" rel="stylesheet">
<!-- Custom CSS -->
<link href="~/css/helper.css" rel="stylesheet">
<link href="~/css/style.css" rel="stylesheet">
@*<script src="~/js/lib/jquery/jquery.min.js"></script>*@

<!-- Start Page Content -->
@*<div class="row">*@
<div class="card card-outline-info">
    <div class="card-header">
        <h4 class="m-b-0 text-white">All Trips</h4>
    </div>
    <div class="card">
        <div class="card-body">
            <div class="table-responsive m-t-40">
                <table id="example23" class="display nowrap table table-hover table-striped table-bordered" cellspacing="0" width="100%">
                    <thead>
                        <tr>
                            <th>TripID</th>
                            <th>TripName</th>
                            <th>TripShortDesc</th>
                            <th>CountryName</th>
                            <th>LocationName</th>
                            <th>Price</th>

                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {


                            <tr>
                                <td>@item.TripID</td>
                                <td>@item.TripName</td>
                                <td>@item.TripShortDesc</td>
                                <td>@item.CountryName</td>
                                <td>@item.LocationName</td>
                                <td>@item.Price</td>


                                <td><a href="~/Trip/EditTrip?TripID=@item.TripID">Edit</a></td>
                            </tr>
                        }

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
@*</div>*@



<!-- End PAge Content -->
<!-- End Wrapper -->
<!-- All Jquery -->
