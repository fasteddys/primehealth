@model List<Filteration.Models.Request>
@{
    ViewBag.Title = "TPAClosedRequest";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row mt">
    <div class="col-md-12">
        <div class="content-panel">
            <h4><i class="fa fa-angle-right"></i> Batch Details</h4>
            <table class="table table-striped table-advance table-hover" id="table">






                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.FirstOrDefault().TicketNumber)
                    </th>
                    @*osama's work*@
                    <th>
                        @Html.DisplayNameFor(model => model.FirstOrDefault().NumberOfBatches)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.FirstOrDefault().Month)
                    </th>
                    
                    <th>
                        @Html.DisplayNameFor(model => model.FirstOrDefault().RequestClosedTicketDate)
                    </th>

                @foreach (var item in Model)
                {
                    <tr>



                        <td>
                            @Html.DisplayFor(modelItem => item.TicketNumber)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.NumberOfBatches)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Month)
                        </td>

                        
                        <td>
                            @Html.DisplayFor(modelItem => item.RequestClosedTicketDate)
                        </td>
                       

                    </tr>
                }

            </table>

        </div>
    </div>
</div>

