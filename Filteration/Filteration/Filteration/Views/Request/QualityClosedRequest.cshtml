@model List<Filteration.Models.Request>
@{
    ViewBag.Title = "QualityClosedRequest";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row mt">
    <div class="col-md-12">
        <div class="content-panel">
            <h4><i class="fa fa-angle-right"></i> Closed Requests</h4>
            <table class="table table-striped table-advance table-hover">



                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.FirstOrDefault().TicketNumber)
                    </th>

                    <th>
                        @Html.DisplayNameFor(model => model.FirstOrDefault().NumberOfBatches)
                    </th>


                    <th>
                        @Html.DisplayNameFor(model => model.FirstOrDefault().RequestStatus)
                    </th>



                    
                </tr>

                @foreach (var item in Model)
                {
                    <tr>



                        <td>
                            @Html.DisplayFor(modelItem => item.TicketNumber)
                        </td>

                        <td>
                            @Html.DisplayFor(modelItem => item.NumberOfBatches)
                        </td>

                        <td id="tt">
                            @Html.DisplayFor(modelItem => item.RequestStatus)

                        </td>

                        


                    </tr>
                }
            </table>
        </div>
    </div>
</div>
